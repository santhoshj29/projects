package com.partpay.repository;

import com.partpay.model.entity.*;
import org.springframework.data.jpa.repository.JpaRepository;
import org.springframework.data.jpa.repository.Query;
import org.springframework.data.repository.query.Param;
import org.springframework.stereotype.Repository;

import java.time.LocalDate;
import java.util.List;
import java.util.Optional;

@Repository
public interface TaxTaxTypeRepository extends JpaRepository<TaxTaxType, TaxTaxTypeId> {
    List<TaxTaxType> findByTaxInformationId(Long taxInformationId);
    void deleteByTaxInformationId(Long taxInformationId);
}